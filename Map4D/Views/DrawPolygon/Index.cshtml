@model Map4D.ViewModels.DrawPolygonViewModels
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col s2 ">
        <ul id="cities"></ul>
    </div>
    <div class="col s2 ">
        <ul id="dictricts"></ul>
    </div>
    <div class="col s2 ">
        <ul id="wards"></ul>
    </div>
    <div class="col s6">
        <div id="xinkciti-map" style="height: 100%; width: 50%; position: absolute;"></div>
    </div>

</div>
<a id="menu-toggle" href="#" class="btn btn-primary btn-lg toggle"><i class="glyphicon glyphicon-bookmark"></i></a>
<div id="sidebar-wrapper">
    <ul class="sidebar-nav">
        <a id="menu-close" href="#" class="btn btn-default btn-lg pull-right toggle"><i class="glyphicon glyphicon-remove"></i></a>
        <li class="sidebar-brand">
            <a href="#">Project name</a>
        </li>
        <li>
            <a href="#">Home</a>
        </li>
        <li>
            <a href="#about">About</a>
        </li>
        <li>
            <a href="#contact">Contact</a>
        </li>
    </ul>
</div>

<script id="city-template" type="x-tmpl-mustache">
    <li class="ul-list">
        <a href="#" aria-expanded="true" class="polygonItems" data-city="{{cityId}}" data-id="{{code}}">{{name}}</a>
    </li>
</script>
<script id="dictrict-template" type="x-tmpl-mustache">
    <li class="ul-list">
        <a href="#" aria-expanded="true" class="polygonItems-dictrict" data-dictrict="{{dictrictId}}" data-id="{{code}}">{{name}}</a>
    </li>
</script>
<script id="ward-template" type="x-tmpl-mustache">
    <li class="ul-list">
        <a href="#" aria-expanded="true" class="polygonItems-ward" data-ward="{{wardId}}" data-id="{{code}}">{{name}}</a>
    </li>
</script>
