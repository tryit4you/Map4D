@model Map4D.ViewModels.DrawPolygonViewModels
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section scripts{
    <script src="~/Assets/js/drawpolygon.js"></script>
}

<div class="row">
    <div class="col-sm-3 col-md-3 col-lg-3" >
        @{ Html.RenderAction("GetAllDataPartial", "DrawPolygon");}
    </div>
 
    <div class="col-sm-7 col-md-7 col-lg-7">
        <div id="xinkciti-map" style="height: 533px; width: 95%; position: absolute;"></div>
    </div>
    <div class="col-sm-2 col-md-2 col-lg-2">
        <table id="popup" class="table" style="margin-left:-25px"></table>
    </div>
</div>
